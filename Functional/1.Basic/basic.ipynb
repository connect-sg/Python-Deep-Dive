   "id": "a5ca2183",
   "metadata": {},
   "source": [
    "Implicit & Explicit\n",
    "\n",
    "Implicit:\n",
    "    [] used for list\n",
    "    () used for tuple\n",
    "    {} used for dictionary\n",
    "    \n",
    "    function arguments/paramters\n",
    "    \n",
    "\n",
    "Explicit:\n",
    "    \\ backslash \n",
    "    "
   ]
